{
  "name": "ramodel",
  "version": "0.0.0",
  "description": "Framework for creating reactive models",
  "keywords": [
    "reactive",
    "hooks",
    "model"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Vladislav Orlov <me@orlov-vo.ru>",
  "homepage": "https://github.com/orlov-vo/ramodel#readme",
  "repository": "github:orlov-vo/ramodel",
  "bugs": {
    "url": "https://github.com/orlov-vo/ramodel/issues",
    "email": "me@orlov-vo.ru"
  },
  "license": "MIT",
  "files": [
    "dist/**/*",
    "hooks/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn test && yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "idx": "^2.5.6"
  }
}
